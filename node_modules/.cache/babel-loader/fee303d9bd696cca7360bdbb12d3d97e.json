,uBAkBA,CAlBA,EAkBA;AACA,MAAA,CAAA,CAAA,cAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,CAAA,CAAA,MAAA;;AACA,UAAA,KAAA,QAAA,EAAA;AACA,QAAA,KAAA,CACA,GADA,sCACA,KAAA,MADA,GACA;AACA,UAAA,IAAA,EAAA,KAAA,IADA;AAEA,UAAA,MAAA,EAAA,KAAA,MAFA;AAGA,UAAA,GAAA,EAAA,KAAA,GAHA;AAIA,UAAA,OAAA,EAAA,KAAA;AAJA,SADA,EAOA,IAPA,CAOA,UAAA,QAAA;AAAA,iBAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA;AAAA,SAPA;AAQA,OATA,MASA;AACA,QAAA,KAAA,CACA,IADA,CACA,4BADA,EACA;AACA,UAAA,IAAA,EAAA,KAAA,IADA;AAEA,UAAA,MAAA,EAAA,KAAA,MAFA;AAGA,UAAA,GAAA,EAAA,KAAA,GAHA;AAIA,UAAA,OAAA,EAAA,KAAA;AAJA,SADA,EAOA,IAPA,CAOA,UAAA,QAAA;AAAA,iBAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA;AAAA,SAPA;AAQA;AACA;AAxCA,GAvBA;AAiEA,EAAA,OAjEA,qBAiEA;AACA,QAAA,KAAA,QAAA,EAAA;AACA,WAAA,IAAA,GAAA,KAAA,QAAA;AACA,WAAA,MAAA,GAAA,KAAA,UAAA;AACA,WAAA,GAAA,GAAA,KAAA,OAAA;AACA,WAAA,OAAA,GAAA,KAAA,WAAA;AACA,KALA,MAKA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,MAAA,GAAA,EAAA;AACA,WAAA,GAAA,GAAA,CAAA;AACA,WAAA,OAAA,GAAA,CACA;AACA,QAAA,IAAA,EAAA,EADA;AAEA,QAAA,KAAA,EAAA,CAFA;AAGA,QAAA,KAAA,EAAA;AAHA,OADA,CAAA;AAOA;AACA;AAnFA,CAAA","sourcesContent":["<template>\n  <div class=\"container\">\n    <form method=\"POST\">\n      <div class=\"form-group\">\n        <label class=\"text-primary\" for=\"code\">Code</label>\n        <input\n          type=\"text\"\n          class=\"form-control col-6\"\n          id=\"input-code\"\n          v-model=\"code\"\n          placeholder=\"Insert the code\"\n        />\n        <small\n          id=\"codeHelp\"\n          class=\"form-text text-muted\"\n        >Make sure your code is bound at the game before saving it</small>\n      </div>\n      <div class=\"form-group\">\n        <label class=\"text-primary\" for=\"inputServer\">Server</label>\n        <select class=\"form-control col-4\" id=\"inputServer\" v-model=\"server\">\n          <option class=\"text-primary\" disabled value>Choose Server...</option>\n          <option class=\"text-primary\" value=\"NA\">NA</option>\n          <option class=\"text-primary\" value=\"JP\">JP</option>\n        </select>\n        <span class=\"text-secondary\">Selected: {{ server }}</span>\n      </div>\n      <div class=\"form-group\">\n        <label for=\"inputSSR\" class=\"text-primary\">SSR Amount</label>\n        <input type=\"number\" class=\"form-control col-3\" id=\"input-ssr\" v-model.number=\"ssr\" />\n        <small\n          id=\"ssrHelp\"\n          class=\"form-text text-muted\"\n        >Insert SSR amount ignoring NP level, you will insert on notable SSR.</small>\n      </div>\n      <label for=\"inputNotable\" class=\"text-primary\">Notable Servants</label>\n      <div class=\"formNotable\">\n        <div v-for=\"(servo, i) in notable\" class=\"row\" :key=\"i\">\n          <div class=\"col-6\">\n            <label for=\"servantName\" class=\"text-primary\">Name</label>\n            <input\n              type=\"text\"\n              class=\"form-control\"\n              v-model=\"notable[i].name\"\n              placeholder=\"Insert Servant Name\"\n            />\n          </div>\n          <div class=\"col-2\">\n            <label for=\"servantLevel\" class=\"text-primary\">Level</label>\n            <input type=\"number\" class=\"form-control\" v-model.number=\"notable[i].level\" />\n          </div>\n          <div class=\"col-2\">\n            <label for=\"servantNp\" class=\"text-primary\">NP Level</label>\n            <input type=\"number\" class=\"form-control\" v-model.number=\"notable[i].npLvl\" />\n          </div>\n          <div class=\"col-2\">\n            <button\n              v-if=\"i == notable.length - 1\"\n              data-toggle=\"tooltip\"\n              title=\"Add more notable Servants\"\n              class=\"btn btn-success btn-addServant\"\n              @click=\"inserirNotable\"\n            >+</button>\n          </div>\n        </div>\n      </div>\n      <div class=\"mt-2\">\n        <button type=\"submit\" class=\"btn btn-primary align-self-center\" @click=\"saveAccount\">Save</button>\n      </div>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"Form\",\n  props: {\n    propId: Number,\n    propCode: String,\n    propServer: String,\n    propSsr: Number,\n    propNotable: Array\n  },\n  data() {\n    return {\n      code: \"\",\n      server: \"\",\n      ssr: 1,\n      notable: [\n        {\n          name: \"\",\n          level: 0,\n          npLvl: 1\n        }\n      ]\n    };\n  },\n  methods: {\n    mostrarDados(e) {\n      e.preventDefault();\n      console.log({\n        code: this.code,\n        server: this.server,\n        ssr: this.ssr,\n        notable: this.notable\n      });\n    },\n    inserirNotable(e) {\n      e.preventDefault();\n      this.notable.push({\n        name: \"\",\n        level: 0,\n        npLvl: 1\n      });\n    },\n    saveAccount(e) {\n      e.preventDefault();\n      console.log(e.target);\n      if (this.propCode) {\n        axios\n          .put(`http://localhost:3000/data/${this.propId}`, {\n            code: this.code,\n            server: this.server,\n            ssr: this.ssr,\n            notable: this.notable\n          })\n          .then(response => console.log(response));\n      } else {\n        axios\n          .post(\"http://localhost:3000/data\", {\n            code: this.code,\n            server: this.server,\n            ssr: this.ssr,\n            notable: this.notable\n          })\n          .then(response => console.log(response));\n      }\n    }\n  },\n  created() {\n    if (this.propCode) {\n      this.code = this.propCode;\n      this.server = this.propServer;\n      this.ssr = this.propSsr;\n      this.notable = this.propNotable;\n    } else {\n      this.code = \"\";\n      this.server = \"\";\n      this.ssr = 1;\n      this.notable = [\n        {\n          name: \"\",\n          level: 0,\n          npLvl: 1\n        }\n      ];\n    }\n  }\n};\n</script>\n\n<style>\n.btn-addServant {\n  margin-top: 31px;\n}\n</style>"],"sourceRoot":"src/components"}]}